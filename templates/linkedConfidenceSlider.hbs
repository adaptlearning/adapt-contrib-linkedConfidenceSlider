{{! Maintainers - Kev Adsett, Himanshu Rajotia}}
<div class="linkedConfidenceSlider-inner component-inner" role="region" aria-label="{{_globals._components._linkedConfidenceSlider.ariaRegion}}" {{#if _globals._components._linkedConfidenceSlider.ariaRegion}}tabindex="0"{{/if}}>
    {{> component this}}
    <div class="linkedConfidenceSlider-widget component-widget{{#unless _isEnabled}} disabled{{#if _isComplete}} complete submitted{{/if}}{{/unless}}">
        <div class="linkedConfidenceSlider-container clearfix">
            {{#if axisLabel}}<div class="linkedConfidenceSlider-label-axis">{{{axisLabel}}}</div>{{/if}}
            <div class="linkedConfidenceSlider-scale clearfix">
                <div class="linkedConfidenceSlider-scale-labels clearfix">
                    {{#if _scale.labels.low}}<div class="linkedConfidenceSlider-scale-label linkedConfidenceSlider-scale-label-low" tabindex="0" role="region">{{{_scale.labels.low}}}</div>{{/if}}
                    {{#if _scale.labels.high}}<div class="linkedConfidenceSlider-scale-label linkedConfidenceSlider-scale-label-high" tabindex="0" role="region">{{{_scale.labels.high}}}</div>{{/if}}
                </div>
                <div class="linkedConfidenceSlider-scale-numbers clearfix">
                    {{#each _items}}
                    <button tabindex="0" role="button" class="base linkedConfidenceSlider-scale-number display-none" data-id="{{value}}">{{value}}</button>
                    {{/each}}
                </div>
                <div class="linkedConfidenceSlider-scale-notches clearfix">
                    {{#each _items}}
                    <div class="linkedConfidenceSlider-scale-notch component-item-color display-none"></div>
                    {{/each}}
                    <div class="linkedConfidenceSlider-scale-background component-item-color"></div>
                </div>
            </div>
            <div class="linkedConfidenceSlider-item item clearfix">
                <div class="linkedConfidenceSlider-item-bar">
                    <div class="linkedConfidenceSlider-item-bar-background"></div>
                    <div class="linkedConfidenceSlider-item-linked-confidence-bar"></div>
                    <div class="linkedConfidenceSlider-item-indicator-bar"></div>
                    <button class="base linkedConfidenceSlider-item-handle component-item-color component-item-text-color  a11y-ignore" tabindex="-1" aria-hidden="true"></button>
                </div>
                <div class="linkedConfidenceSlider-item-outer-bar"></div>
            </div>
        </div>
    </div>
    <div class="buttons">
    </div>
</div>
